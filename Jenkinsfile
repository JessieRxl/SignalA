pipeline {
  agent any
  stages {
    stage('stage1') {
      parallel {
        stage('stage1') {
          steps {
            sh 'sfsdfsdf'
            dingtalk 'sdfsdfsdf'
          }
        }

        stage('stage2') {
          steps {
            sh 'ssdfsdf'
            sh 'sdfsdfsfd'
          }
        }

      }
    }

    stage('sssss') {
      parallel {
        stage('sssss') {
          steps {
            sh 'ddddd'
          }
        }

        stage('sssssdddddd') {
          steps {
            sh 'ddddddd'
          }
        }

      }
    }

  }
}